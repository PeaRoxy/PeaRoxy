<?xml version="1.0" encoding="utf-8"?>
<SmartProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <HttpRules>
    <string>*google.c* | *</string>
    <string>*facebook.c* | *</string>
    <string>*youtube.c* | *</string>
    <string>*twitter.c* | *</string>
    <string>*paypal* | *</string>
    <string>*amazon.c* | *</string>
  </HttpRules>
  <DirectRules>
    <string>*google.c*:* | *</string>
    <string>*facebook.c*:* | *</string>
    <string>*youtube.c*:* | *</string>
    <string>*twitter.c*:* | *</string>
    <string>*paypal*:* | *</string>
    <string>*amazon.c*:* | *</string>
  </DirectRules>
  <AntiDnsPoisoningRule>^(10.10.*.*)$</AntiDnsPoisoningRule>
  <AntiBlockPageRule>^HTTP/1.1 403 Forbidden(\r\nConnection:close)*</AntiBlockPageRule>
  <DirectTimeout>20</DirectTimeout>
  <IsHttpSupported>true</IsHttpSupported>
  <IsHttpsSupported>true</IsHttpsSupported>
  <IsSocksSupported>false</IsSocksSupported>
  <HttpBlockPageDetection>true</HttpBlockPageDetection>
  <DnsPoisoningDetection>true</DnsPoisoningDetection>
  <TimeoutDetection>true</TimeoutDetection>
  <TreatPort80AsHttp>true</TreatPort80AsHttp>
</SmartProfile>